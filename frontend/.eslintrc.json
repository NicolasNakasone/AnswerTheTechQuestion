{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "no-unused-vars": ["warn", { "vars": "all" }],
    "prefer-const": ["warn", {
        "destructuring": "any",
        "ignoreReadBeforeAssign": false
    }],
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "import/no-unresolved": 0,
    "linebreak-style": "off",
    "semi": ["error", "always"],
    "object-curly-newline": ["error", { "ImportDeclaration": "never", "ExportDeclaration": "never" }],
    "comma-dangle": ["error", {
        "arrays": "ignore",
        "objects": "ignore",
        "imports": "ignore",
        "exports": "ignore",
        "functions": "ignore"
    }],
    "max-len": ["error", {
      "code": 150,
      "comments": 150
    }]
  }
}
